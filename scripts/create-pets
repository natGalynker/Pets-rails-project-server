    curl --request POST http://localhost:3000/pets
    --header "Authorization: Token token=$TOKEN" \
    --header "Content-Type: application/json" \
    --data '{
      "pet": {
        "name": "Honey-Bee",
        "breed": "Golden Retreiver",
        "born_on": "2008-04-08",
        "gender": "f",
      }
    }'
